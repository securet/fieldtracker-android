<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="oval">
            <stroke
                android:width="2dp"
                android:color="@android:color/holo_blue_dark" />
            <size
                android:width="15dp"
                android:height="15dp" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
        </shape>
    </item>
    <item
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp"
        android:top="5dp">
        <shape android:shape="oval">
            <solid
                android:width="2dp"
                android:color="@android:color/holo_blue_dark" />
            <size
                android:width="2dp"
                android:height="2dp" />
        </shape>
    </item>
</layer-list>